<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="720.0" prefWidth="1280.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="client.view.LoginMenu">

    <stylesheets>
        <URL value="@../CSS/RegisterMenu.css" />
    </stylesheets>

    <children>
        <Button layoutX="495.0" layoutY="575.0" mnemonicParsing="false" onMouseClicked="#goToRegisterMenu" prefHeight="46.0" prefWidth="290.0" text="back to register menu" />
        <Label alignment="CENTER" layoutX="552.0" layoutY="182.0" prefHeight="31.0" prefWidth="175.0" text="Login menu" />
        <TextField fx:id="username" alignment="CENTER" layoutX="515.0" layoutY="237.0" prefHeight="38.0" prefWidth="250.0" promptText="Enter your username" />
        <PasswordField fx:id="password" alignment="CENTER" layoutX="515.0" layoutY="293.0" prefHeight="47.0" prefWidth="250.0" promptText="Enter your password" />
        <Button alignment="CENTER" layoutX="515.0" layoutY="368.0" mnemonicParsing="false" onMouseClicked="#login" prefHeight="76.0" prefWidth="250.0" text="login" />
        <Button alignment="CENTER" layoutX="495.0" layoutY="461.0" mnemonicParsing="false" onMouseClicked="#forgetPassword" prefHeight="47.0" prefWidth="290.0" text="forget your password?" />
    </children>
</AnchorPane>
